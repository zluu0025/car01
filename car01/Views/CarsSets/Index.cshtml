@model IEnumerable<car01.Models.CarsSet>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LocationSet.Location_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Car_Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Car_MarK)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Car_Vin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Car_Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Car_License_Plate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LocationSet.Location_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Car_Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Car_MarK)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Car_Vin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Car_Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Car_License_Plate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Car_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Car_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Car_Id })
        </td>
    </tr>
}

</table>
